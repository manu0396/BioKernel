CREATE TABLE RetinaEntity (
    id TEXT NOT NULL PRIMARY KEY,
    patientId TEXT NOT NULL,
    score REAL NOT NULL,
    toxicity TEXT NOT NULL,
    timestamp INTEGER NOT NULL,
    notes TEXT
);

insertAnalysis:
INSERT OR REPLACE INTO RetinaEntity(id, patientId, score, toxicity, timestamp, notes)
VALUES (?, ?, ?, ?, ?, ?);

selectAll:
SELECT * FROM RetinaEntity WHERE patientId = ?;

deleteByPatient:
DELETE FROM RetinaEntity WHERE patientId = ?;